<div id="table-two">
		<h3><%=@boat.name%>'s Job Log</h3>
	<table>
		<thead>
		<tr>
			<th>Job Name</th>
			<th>Origin</th>
			<th>Destination</th>
			<th>Shipment Value</th>
			<th>Note</th>
		</tr>
	</thead>
	<tbody>
		<% @jobs.each do |job| %>
		<tr>
			<td><%=job.name%></td>
			<td><%=job.origin%></td>
			<td><%=job.destination%></td>
			<td><%=job.cost%></td>
			<td><%=job.note%></td>	
			<%end%>
	</tr>
	</tbody>
</table>
</div>


