<%= render 'home/index' %>

<div class="login_form">
	<%= form_tag login_path do%>
		<%#= label_tag("Email") %>
		<%= text_field_tag(:email, nil, placeholder: 'email') %>
		<br>

		<%#= label_tag ("Password") %>
		<%= password_field_tag(:password, nil, placeholder: 'password') %> 
		
	
		<div class="login_btn">
			<%=submit_tag "Login" %>
		</div>
		</div>
	<%end%>




