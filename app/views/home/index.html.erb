<%#= render 'home/index' %>
	<div class="login_form">
		<%= form_tag login_path do%>
		<%= text_field_tag(:email, nil, placeholder: 'email') %>
		<%= password_field_tag(:password, nil, placeholder: 'password') %>
	</div>	
	
	<div class="login_btn">
		<%=submit_tag "Login" %>
		<br>
		<h4>OR</h4>
		<br>
	<%end%>
	</div>

