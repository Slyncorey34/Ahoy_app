<!-- render<h2>Login</h2> -->
<div class="form">
	<%= form_tag login_path do%>
		<%= label_tag("Email") %>
		<%= text_field_tag(:email) %>
		<%= label_tag ("Password") %>
		<%= password_field_tag(:password) %> 
		<%=submit_tag "Login" %>
<%end%>
</div>

<h2> Or</h2><%= link_to 'Sign Up', new_user_path%>
